{
  "name": "@awesome-algorand/mdast",
  "version": "0.0.1",
  "description": "Manage the Awesome Algorand README",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "browser": "./dist/esm.js",
  "type": "module",
  "files": [
    "src",
    "lib"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./root": {
      "types": "./lib/root.d.ts",
      "default": "./lib/root.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.app.json",
    "docs": "typedoc",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:style": "eslint",
    "lint:format": "prettier . --check",
    "bench": "vitest bench",
    "test": "concurrently \"npm:test:*\"",
    "test:cov": "vitest run --coverage",
    "release": "semantic-release"
  },
  "author": "Awesome Algorand",
  "license": "Unlicense",
  "devDependencies": {
    "@codecov/vite-plugin": "^1.9.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@semantic-release/npm": "^12.0.0",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-istanbul": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.8",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "glob": "^11.0.1",
    "micromark": "^4.0.2",
    "playwright": "^1.51.0",
    "semantic-release": "^23.0.8",
    "semantic-release-gitmoji": "^1.6.8",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-bundle-analyzer": "^0.18.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "3.0.9"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-heading-range": "^4.0.0",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "mdast-util-toc": "^7.1.0",
    "unist-util-is": "^6.0.0",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1"
  },
  "release": {
    "plugins": [
      "semantic-release-gitmoji",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "ðŸ”– [skip ci] @awesomealgorand/mdast \n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ],
    "branches": [
      "release/*",
      {
        "name": "main",
        "prerelease": "canary"
      }
    ]
  }
}
